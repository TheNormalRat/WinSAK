{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell: Launch Start-WinSAK.ps1",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/Start-WinSAK.ps1",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "PowerShell: Launch Current Script",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "cwd": "${file}"
        },
        {
            "name": "PowerShell: Interactive Session",
            "type": "PowerShell",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "createTemporaryIntegratedConsole": false
        },
        {
            "name": "PowerShell: Test Module Import",
            "type": "PowerShell",
            "request": "launch",
            "script": "${workspaceFolder}/WindowsSwissArmyKnife.psm1",
            "cwd": "${workspaceFolder}",
            "createTemporaryIntegratedConsole": false,
            "preLaunchTask": null
        }
    ]
}
